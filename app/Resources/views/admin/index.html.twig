{# app/Resources/views/lucky/number.html.twig #}
{% extends 'base.html.twig' %}

{% block body %}

{{ form_start(form) }}
{{ form_widget(form) }}
{{ form_end(form) }}

<table class="table">
<tr>
    <th>なまえ</th>
    <td>メール</td>
    <td>電話番号</td>
    <td>編集</td>
    <td>削除</td>
</tr>
{% for inquiry in inquiryList %}
<tr>
    <th>{{ inquiry.name }}</th>
    <td>{{ inquiry.email }}</td>
    <td>{{ inquiry.tel }}</td>
    <td><a href={{ path('inquiry.get', {"id": inquiry.id}) }}>編集</a></td>
    <td><a href={{ path('inquiry.remove', {"id": inquiry.id}) }}>削除</a></td>
</tr>
{% endfor %}
</table>

{% endblock %}